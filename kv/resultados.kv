<Resultados>
    BoxLayout:
        spacing: 10
        padding: 10
        orientation: "vertical"
        
        MDFloatLayout:

            id: tablas
            size_hint: 1,1
            size_hint: 1,.85
            #adaptive_height: True
            md_bg_color:  0, .4, 0, 0.1
            #orientation: "vertical"
            canvas:
                Color:
                    rgba: .04, .33, .27, 1
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius:[(20.0, 20.0), (20.0, 20.0), (20.0, 20.0), (20.0, 20.0)]
            MDLabel:
                id: mensaje
                pos_hint: {"center_x": .5, "center_y": .5}
                size_hint: (.8, .1)
                theme_text_color: "Primary"
                font_style: "H5"
                font_name : "Urban Class"
                theme_text_color: "Custom"
                text_color: 1, 1, 1, 1
                halign: "center"
                text: "resultados en la tablas de multiplicar"

        BoxLayout:
            padding: 20
            spacing:20
            size_hint: 1,.15
            canvas:
                Color:
                    rgba: .46, .79, .73, 0.1
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius:[(20.0, 20.0), (20.0, 20.0), (20.0, 20.0), (20.0, 20.0)]
                
            MDFloatLayout:
                # MDFillRoundFlatButton:

                #     pos_hint: {"x":0.1, "center_y":.5}
                #     #size_hint: 0.3, None
                #     id: clear
                #     text: "Clear"
                #     theme_text_color: "Custom"
                #     text_color: 1, 1, 1, 1
                #     md_bg_color: 0.3, 0.3, .3, 1
                #     font_style: "H6"
                #     on_release: root.clear_canvas_tablas()

                MDFillRoundFlatButton:

                    pos_hint: {"x":0.1, "center_y":.5}
                    #size_hint: 0.3, None
                    id: resultados
                    text: "Resultados"
                    theme_text_color: "Custom"
                    text_color: 1, 1, 1, 1
                    md_bg_color: 0.3, 0.3, .3, 1
                    font_style: "H6"
                    on_release: root.tabla_resultados()
                
                MDFillRoundFlatButton:

                    pos_hint: {"x":0.55, "center_y":.5}
                    #size_hint: 0.3, None
                    id: estadisticas
                    text: "Estadisticas"
                    theme_text_color: "Custom"
                    text_color: 1, 1, 1, 1
                    md_bg_color: 0.3, 0.3, .3, 1
                    font_style: "H6"
                    on_release: root.tabla_estadisticas()

